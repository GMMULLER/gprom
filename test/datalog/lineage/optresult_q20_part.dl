PROV_PART(V1,V2,V3,V4,V5,V6,V7,V8,V9) :-
    PART(V1,V2,V3,V4,V5,V6,V7,V8,V9),PARTSUPP(V1,S_SUPPKEY,V10,V11,V12),
    Q1(V1,S_SUPPKEY,V13),SUPPLIER(S_SUPPKEY,S_NAME,S_ADDRESS,N_NATIONKEY,S1,S2,S3),
    NATION(N_NATIONKEY,'CANADA',N1,N2),
    ((V10 > V13)),
    ((V2 LIKE 'forest%')),
    RTPCQ20(S_NAME,S_ADDRESS).

Q1(L_PARTKEY,L_SUPPKEY,sum((0.500000 * L_QUANTITY))) :-
    LINEITEM(L1,L_PARTKEY,L_SUPPKEY,L2,L_QUANTITY,L3,L4,L5,L6,L7,L_SHIPDATE,L8,L9,L10,L11,L12),
    ((L_SHIPDATE >= '1994-01-01')),
    ((L_SHIPDATE < '1995-01-01')).

ANS: PROV_PART.
