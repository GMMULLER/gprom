<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Temporal Sequenced Rewrites Test Cases for Aggregation queries</comment>

<entry key="q1.query">
TEMPORAL (SELECT count(*) AS BOO FROM TEMP_TEST WITH TIME(T_BEGIN, T_END));
</entry>
<entry key="q1.result">
 BOO | T_B | T_E |
------------------
 0   | -2000000000 | 1          
 4   | 1   | 4   |
 3   | 4   | 6   |
 2   | 6   | 10  |
 1   | 10  | 13  |
 0   | 13  | 2000000000 |
</entry>


<entry key="q2.query">
TEMPORAL (SELECT count(*) AS BOO, A FROM TEMP_TEST WITH TIME(T_BEGIN, T_END) GROUP BY A);
</entry>
<entry key="q2.result">
 BOO | A | T_B | T_E |
----------------------
 3   | 1 | 1   | 6   |
 2   | 1 | 6   | 10  |
 1   | 1 | 10  | 13  |
 1   | 2 | 1   | 4   |
</entry>

<entry key="q3.query">
TEMPORAL (SELECT count(*) FROM (SELECT count(*) AS BOO, A FROM TEMP_TEST WITH TIME(T_BEGIN, T_END) GROUP BY A) a);
</entry>
<entry key="q3.result">
 COUNT(1) | T_B | T_E |
-----------------------
 0        | -2000000000 | 1          |
 1        | 4   | 13  |
 2        | 1   | 4   |
 0        | 13          | 2000000000 |
</entry>

<entry key="q4.query">
TEMPORAL (SELECT count(1) FROM TEMP_DATE WITH TIME(T_BEGIN, T_END));
</entry>
<entry key="q4.result">
 COUNT(1) | T_B                   | T_E                   |
----------------------------------------------------
 0        | 4712-01-01 00:00:00.0 | 2017-01-01 00:00:00.0 |
 1        | 2017-01-01 00:00:00.0 | 2017-01-05 00:00:00.0 |
 2        | 2017-01-05 00:00:00.0 | 2017-02-15 00:00:00.0 |
 1        | 2017-02-15 00:00:00.0 | 2017-04-07 00:00:00.0 |
 0        | 2017-04-07 00:00:00.0 | 9999-01-01 00:00:00.0 |
</entry>


<entry key="q5.query">
TEMPORAL (SELECT min(B) AS BOO, A FROM TEMP_TEST WITH TIME(T_BEGIN, T_END) GROUP BY A);
</entry>
<entry key="q5.result">
 BOO | A | T_B | T_E |
----------------------
 1   | 1 | 1   | 10  |
 2   | 1 | 10  | 13  |
 1   | 2 | 1   | 4   |
</entry>

<entry key="q6.query">
TEMPORAL (SELECT max(B) AS BOO, A FROM TEMP_TEST WITH TIME(T_BEGIN, T_END) GROUP BY A);
</entry>
<entry key="q6.result">
 BOO | A | T_B | T_E |
----------------------
 1   | 2 | 1   | 4   |
 2   | 1 | 1   | 13  |
</entry>

